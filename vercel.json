{
  "rewrites": [
    {
      "source": "/metaAnalysis",
      "destination": "/"
    },
    {
      "source": "/metaAnalysis/:path*",
      "destination": "/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
} 